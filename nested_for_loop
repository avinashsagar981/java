public class LoopExample {
    public static void main(String[] args) {
        
        // Outer loop
        for(int i = 1; i <= 3; i++) {
            System.out.println("Outer loop i = " + i);
            
            // Inner loop
            for(int j = 1; j <= 2; j++) {
                System.out.println("    Inner loop j = " + j);
            }
        }
    }
}
