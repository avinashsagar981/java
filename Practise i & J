public class NestedLoopExample {
    public static void main(String[] args) {
        // Outer loop runs 3 times
        for (int i = 1; i <= 3; i++) {
            System.out.println("i = " + i);  // Print current value of i

            // Inner loop runs 2 times for each i
            for (int j = 1; j <= 2; j++) {
                System.out.println("   j = " + j);  // Print current value of j (indented)
            }

            System.out.println(); // Print a blank line after each outer loop
        }
    }
}
